net: "TVG_CRFRNN_new_traintest.prototxt"

test_iter: 500
test_interval: 2000

display: 500

lr_policy: "fixed"
base_lr: 1e-13
momentum: 0.99

weight_decay: 0.0005

# The maximum number of iterations, DO NOT INCREASE THIS FURTHER
max_iter: 1000000000

# snapshot intermediate results
snapshot: 2000
snapshot_prefix: "/YOURPATH/crfrnn-" ##CHANGE THIS

# solver mode: CPU or GPU
solver_mode: GPU


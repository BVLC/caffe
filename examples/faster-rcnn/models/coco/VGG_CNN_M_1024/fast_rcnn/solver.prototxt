train_net: "models/coco/VGG_CNN_M_1024/fast_rcnn/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 200000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg_cnn_m_1024_fast_rcnn"
#debug_info: true

{
  "env" : {
    "node" : true
  },
  "rules" : {
    "curly" : 0,
    "no-lonely-if" : 1,
    "no-mixed-requires" : 0,
    "no-underscore-dangle" : 0,
    "no-unused-vars" : [2, {"vars" : "all", "args" : "after-used"}],
    "no-use-before-define" : [2, "nofunc"],
    "quotes" : [1, "double", "avoid-escape"],
    "semi" : [2, "never"],
    "space-after-keywords" : 1,
    "space-infix-ops" : 0,
    "strict" : 0
  }
}

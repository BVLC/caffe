{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "smarttabs": false,
  "maxlen": 80,
  "maxdepth": 2,
  "predef": [
    "define",
    "require",
    "describe",
    "it",
    "xit",
    "before",
    "beforeEach",
    "after",
    "afterEach"
  ]
}

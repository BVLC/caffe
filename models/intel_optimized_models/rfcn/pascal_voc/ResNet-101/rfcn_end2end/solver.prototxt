train_net: "../../models/intel_optimized_models/rfcn/pascal_voc/ResNet-101/rfcn_end2end/train_agnostic.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 80000
display: 1

momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "resnet101_rfcn"
iter_size: 2
# debug_info: true
solver_mode: CPU
# time_info: true
